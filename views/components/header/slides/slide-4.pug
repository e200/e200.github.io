div.v-align(class="slide-" + i)
    .v-wrapper.text-center
        h4.full-name
            span #{full_name}

        include ../nav/nav

        ul.social.list-unstyled
            each social, index in socials
                li
                    a(href=social.address title=index)
                        i.fa(class="fa-" + social.icon)